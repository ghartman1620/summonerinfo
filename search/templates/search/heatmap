	var heatmapInstance = h337.create({
		container: document.querySelector('.killheatmap')
	});
	var points = [];
	//If anybody posts this to /r/badcode I'll be salty. It's generated. Pls leave me alone.
	var point = {
		x: 0
		y: 0
		val:0
	};
	{% for kill in championKills %}
	point = {
		x: {{kill.x}},
		y: {{kill.y}},
		value: 10
	};
	points.push(point);
	{% endfor %}
	var data = {
		max: 10,
		data: points
	}
	heatmapInstance.setData(data);
	{% endcomment %}